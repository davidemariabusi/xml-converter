{
    "name": "dmb/xml-converter",
    "description": "A package that allows you to easily convert your XML string into array format or generate an XML string from an array.",
    "keywords": [
        "dmb",
        "xml",
        "converter",
        "array",
        "php"
    ],
    "homepage": "https://github.com/davidemariabusi/xml-converter",
    "license": "MIT",
    "authors": [
        {
            "name": "Davide Maria Busi",
            "email": "davidemaria.busi@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/array-to-xml": "^3.4.4"
    },
    "require-dev": {
        "pestphp/pest": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Dmb\\XmlConverter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dmb\\XmlConverter\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dmb\\XmlConverter\\XmlConverterServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
